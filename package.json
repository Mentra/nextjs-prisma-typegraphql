{
  "name": "nextjs-prisma-typegraphql",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@prisma/client": "3.0.1",
    "@types/cookies": "^0.7.7",
    "apollo-server-micro": "^3.3.0",
    "babel-plugin-superjson-next": "^0.4.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "cookies": "^0.8.0",
    "graphql": "^15.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.10.1",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "next": "11.1.2",
    "prisma": "3.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "superjson": "^1.7.5",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.16.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/micro": "^7.3.6",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.2.1",
    "ts-node": "^10.2.1",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=14.15.3"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "next build",
    "compile": "npx prisma generate",
    "db:create": "docker run --detach --publish 5432:5432 --name prismadb -e POSTGRES_PASSWORD=prisma postgres:12",
    "db:migrate": "env-cmd -f .env.development.local --use-shell 'prisma migrate dev'",
    "db:remove": "docker rm prismadb",
    "db:reset": "npm run db:stop && npm run db:remove && npm run db:create",
    "db:seed": "env-cmd -f .env.development.local --use-shell 'npx prisma db seed'",
    "db:start": "docker start prismadb",
    "db:stop": "docker stop prismadb",
    "dev": "next dev",
    "format": "npm run format:js && npm run format:css;",
    "format:css": "prettier --config .prettierrc --write ./pages ./lib ./components",
    "format:js": "eslint --config .eslintrc --fix --quiet ./pages ./lib ./components",
    "lint": "eslint --config .eslintrc --quiet ./pages ./lib ./components",
    "start": "next start"
  }
}
